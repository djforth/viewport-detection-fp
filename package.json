  "license": "MIT",
  "scripts": {
    "build": "BABEL_ENV=production babel src -x '.js','.jsx' -d lib",
    "build:watch": "BABEL_ENV=production  babel src -x '.js','.jsx' -w --source-maps -d lib",
    "js": "webpack --config config/webpack/production.js",
    "lint": "eslint src/** --fix",
    "lint:tests": "eslint __tests__/** --fix",
    "server": "NODE_ENV=development webpack-dev-server --progress --color --config config/webpack/development.js",
    "start": "yarn run server",
    "test": "jest --coverage --config jest.config.json",
    "test:watch": "jest --watch --config jest.config.json"
  },
  "devDependencies": {
    "@djforth/jest-call-helpers": "^1.1.0",
    "@djforth/jest-matchers": "^1.2.0",
    "@djforth/stubs-spy-manager-jest": "^1.1.1",
    "@morsedigital/jest-config": "^1.0.0",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-rewire-ignore-coverage": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-config-morsedigital-jest": "^2.0.0",
    "immutable": "^3.8.1",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "moment": "^2.18.1"
  },
  "dependencies": {
    "babel-cli": "^6.24.1"
  }
